
<!-- begin audio player -->
<script type="text/javascript">
  const {{ include.name }} = new Tone.Oscillator({{ include.freq }}, "{{ include.type }}").toDestination();
  {{ include.name }}.onstop = (source) => {transport_{{ audio_ex_ctr}}('stop');};
  transport_fns_{{ audio_ex_ctr }}.play.push(() => {
      {{ include.name }}.start();
  });
  transport_fns_{{ audio_ex_ctr }}.stop.push(() => { {{ include.name }}.stop(); });
  Tone.loaded().then(() => {
  });
</script>
<!-- end audio player -->
